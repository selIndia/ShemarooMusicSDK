<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomButton">
        <attr name="pressed_color" format="color" />
        <attr name="unpressed_color" format="color" />
        <attr name="round_radius" format="dimension" />
        <attr name="btn_radius" format="dimension" />
        <attr name="ripple_color" format="dimension" />
        <attr name="ripple_alpha" format="integer" />
        <attr name="ripple_duration" format="integer" />
        <attr name="shape_type" format="enum">
            <enum name="circle" value="0" />
            <enum name="rectangle" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ShemarooMusicButton">
        <attr name="image_src" format="integer" />
       <!-- <attr name="unpressed_src" format="reference" />-->
    </declare-styleable>
</resources>